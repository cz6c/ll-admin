{
  "name": "@apps/server",
  "private": true,
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate -d ./src/config/typeorm.config.ts ./src/migrations/migration",
    "migration:create": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:create -d ./src/config/typeorm.config.ts ./src/migrations/migration",
    "migration:run": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/config/typeorm.config.ts",
    "migration:run:prod": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/config/typeorm.config.ts",
    "migration:revert": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/config/typeorm.config.ts",
    "migration:revert:prod": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@llcz/common": "^1.0.4",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.1.4",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.4",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.4",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.56.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cos-nodejs-sdk-v5": "2.16.0-beta.3",
    "cron": "4.3.0",
    "cron-parser": "^5.3.0",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.6.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mqtt": "^5.13.2",
    "multer": "2.0.2",
    "mysql2": "3.14.2",
    "nest-winston": "^1.10.2",
    "node-disk-info": "^1.3.0",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path-to-regexp": "^8.2.0",
    "qiniu": "^7.14.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.2",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25",
    "useragent": "^2.3.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.4",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
